<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.test">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HN21_CPL_Android_03G2">
        <activity android:name="com.example.test.setting.PasswordActivity" />
        <activity android:name="com.example.test.setting.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.test.todolist.AddOrEditActivity" />
        <activity android:name="com.example.test.MainActivity"
            android:screenOrientation="portrait">
        </activity>

        <receiver android:name="com.example.test.calendar.timer.NotifyReceiver" />

        <activity android:name="com.example.test.news.CovidActivity" />
        <activity android:name="com.example.test.news.WeatherActivity" />
    </application>

</manifest>